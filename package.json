{
  "name": "spa-jwt-session-utils",
  "version": "1.0.4",
  "description": "utils for jwt and session in spa",
  "type": "module",
  "scripts": {
    "dev": "SET NODE_ENV=development& ts-node src/index.ts",
    "test": "SET NODE_ENV=test& jest",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs . && prettier -c src test",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix && prettier -c src test -w",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test:types": "tsc --noEmit --skipLibCheck"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "repository": "https://github.com/RolandoHidalgo/create-npm-lib",
  "keywords": [
    "spa",
    "jwt",
    "session"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": "skidrow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/RolandoHidalgo/spa-jwt-session-utils/issues"
  },
  "homepage": "https://github.com/RolandoHidalgo/spa-jwt-session-utils#readme",
  "devDependencies": {
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.6.0",
    "changelogen": "^0.5.5",
    "eslint-config-unjs": "^0.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "ofetch": "^1.3.3"
  }
}